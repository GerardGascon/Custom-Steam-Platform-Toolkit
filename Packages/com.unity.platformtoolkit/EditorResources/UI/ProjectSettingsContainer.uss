.title {
    font-size: 20px;
    margin-bottom: 10px;
    margin-top: 5px;
    margin-right: 0;
    margin-left: 10px;
    min-height: 20px;
    -unity-font-style: bold;
}

.unity-group-box__label {
    font-size: var(--unity-metrics-default-font_big_size);
    margin-top: 5px;
    margin-bottom: 25px;
    -unity-font-style: bold;
}

.h2 {
    font-size: var(--unity-metrics-default-font_normal_size);
    -unity-font-style: bold;
    margin: 0;
    width: 200px;
    margin-bottom: 5px;
}

.content {
    flex-grow: 1;
    flex-shrink: 0;
    margin-left: 10px;
}

.section {
    margin-bottom: 25px;
}

.section > .unity-base-field {
    margin-bottom: 5px;
}

.container {
    margin-bottom: 10px;
}
