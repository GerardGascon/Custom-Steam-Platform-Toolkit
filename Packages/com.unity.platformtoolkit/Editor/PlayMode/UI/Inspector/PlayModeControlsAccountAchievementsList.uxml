<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <engine:Foldout text="Achievements">
        <engine:ListView data-source-type="Unity.PlatformToolkit.PlayMode.PlayModeAccountAchievementData, Unity.PlatformToolkit.PlayMode.Editor" name="achievements-list" show-bound-collection-size="false" header-title="Achievements" show-foldout-header="false">
            <Bindings>
                <engine:DataBinding property="itemsSource" data-source-path="Achievements" />
            </Bindings>
        </engine:ListView>
        <engine:Button name="reset-all-button" text="Reset All Achievements" />
    </engine:Foldout>
</engine:UXML>
