<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="project://database/Packages/com.unity.platformtoolkit/Editor/PlayMode/UI/Inspector/Styles.uss?fileID=7433441132597879392&amp;guid=2c5dcac6c00794199998ccd2e7c525b0&amp;type=3#Styles" />
    <engine:VisualElement name="SaveDataInfoField" class="save-field">
        <engine:VisualElement name="SaveStringsElement" data-source-type="Unity.PlatformToolkit.PlayMode.PlayModeSaveDataInfo, Unity.PlatformToolkit.PlayMode.Editor" class="save-field__strings">
            <engine:Label text="SaveName" name="SaveNameLabel" style="text-overflow: ellipsis; flex-wrap: wrap; flex-shrink: 1; overflow: hidden;">
                <Bindings>
                    <engine:DataBinding property="text" data-source-path="Name" data-source-type="Unity.PlatformToolkit.PlayMode.PlayModeSaveDataInfo, Unity.PlatformToolkit.PlayMode.Editor" binding-mode="ToTarget" />
                </Bindings>
            </engine:Label>
        </engine:VisualElement>
        <engine:VisualElement name="SaveButtonsElement" data-source-type="Unity.PlatformToolkit.PlayMode.PlayModeSaveData, Unity.PlatformToolkit.PlayMode.Editor">
            <engine:Button name="SaveImportButton" tooltip="Import to this save" text="Import" class="save-field__button" />
            <engine:Button text="Export" name="SaveExportButton" class="save-field__button" />
            <engine:Button name="SaveDeleteButton" tooltip="Delete the save" text="Delete" />
        </engine:VisualElement>
    </engine:VisualElement>
</engine:UXML>
