<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="project://database/Packages/com.unity.platformtoolkit/Editor/PlayMode/UI/Styles.uss?fileID=7433441132597879392&amp;guid=8d8d414ba2ad6ce4bbfe36e0ed291897&amp;type=3#Styles" />
    <Unity.PlatformToolkit.PlayMode.PlayModeControlsAccountControlsField name="play-mode-controls-account-controls" class="mb-20">
        <engine:Label text="Account Controls" class="title" />
        <engine:VisualElement name="main-container" class="warning-border">
            <engine:VisualElement name="warnings" class="column">
                <engine:HelpBox name="select-account-warning" text="Select the account you would like to use!" message-type="warning" class="m-0 hidden"/>
                <engine:HelpBox name="add-account-error" text="Please add an account to your PlayModeControls asset!" message-type="error" class="m-0 hidden"/>
                <engine:HelpBox name="no-primary-error" text="This configuration requires a primary account! Please select a primary account." message-type="error" class="m-0 hidden"/>
            </engine:VisualElement>
            <engine:VisualElement class="column justify-start">
                <engine:VisualElement class="row">
                    <engine:Label text="Primary" class="subHeading bold wide"/>
                    <engine:Label text="Account Name" class="subHeading bold extra-wide"/>
                    <engine:Label text="Signed-In" class="subHeading bold wide"/>
                    <engine:Label text="Select Account" class="subHeading bold wide hidden"/>
                </engine:VisualElement>
                <engine:VisualElement class="column" name="accounts-list">
                </engine:VisualElement>
                <engine:VisualElement class="row">
                    <engine:VisualElement class="wide"></engine:VisualElement>
                    <engine:VisualElement class="extra-wide"></engine:VisualElement>
                    <engine:VisualElement class="wide"></engine:VisualElement>
                    <engine:Button text="Refuse" name="refuse-button" class="bg-red wide bold hidden"/>
                </engine:VisualElement>
            </engine:VisualElement>
        </engine:VisualElement>
    </Unity.PlatformToolkit.PlayMode.PlayModeControlsAccountControlsField>
</engine:UXML>
