<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="project://database/Packages/com.unity.platformtoolkit/Editor/Achievement/UI/style.uss?fileID=7433441132597879392&amp;guid=73b56bde08676d743a5171a0d5024e43&amp;type=3#style" />
    <engine:VisualElement class="column__cell">
        <engine:VisualElement name="icon-container" class="column__cell__warning-container">
            <engine:VisualElement name="warning-icon" class="column__cell__warning-image">
                <Bindings>
                    <engine:DataBinding property="style.display" data-source-path="Warnings" binding-mode="ToTarget" source-to-ui-converters="Warning Converters" />
                    <engine:DataBinding property="tooltip" data-source-path="Warnings" binding-mode="ToTarget" source-to-ui-converters="Warning Converters" />
                </Bindings>
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement data-source-type="Unity.PlatformToolkit.Editor.StoredAchievement, Unity.PlatformToolkit.Editor" name="VisualElement" class="column__cell__attribute-list">
            <engine:TextField label="Id" is-delayed="true" enabled="true">
                <Bindings>
                    <engine:DataBinding property="value" data-source-path="StoredAchievement.Id" binding-mode="TwoWay" data-source-type="Unity.PlatformToolkit.Editor.CommonCellViewModel, Unity.PlatformToolkit.Editor" />
                </Bindings>
            </engine:TextField>
            <engine:EnumField label="Unlock Type" type="Unity.PlatformToolkit.Editor.UnlockType, Unity.PlatformToolkit.Editor">
                <Bindings>
                    <engine:DataBinding property="value" data-source-path="StoredAchievement.UnlockType" binding-mode="TwoWay" data-source-type="Unity.PlatformToolkit.Editor.CommonCellViewModel, Unity.PlatformToolkit.Editor" />
                </Bindings>
            </engine:EnumField>
            <engine:IntegerField label="Progress Target" value="1" is-delayed="true">
                <Bindings>
                    <engine:DataBinding property="value" data-source-path="StoredAchievement.ProgressTarget" data-source-type="Unity.PlatformToolkit.Editor.CommonCellViewModel, Unity.PlatformToolkit.Editor" binding-mode="TwoWay" />
                    <engine:DataBinding property="style.display" data-source-path="StoredAchievement.UnlockType" data-source-type="Unity.PlatformToolkit.Editor.CommonCellViewModel, Unity.PlatformToolkit.Editor" binding-mode="ToTarget" source-to-ui-converters="Unlock Type Converters" />
                </Bindings>
            </engine:IntegerField>
        </engine:VisualElement>
    </engine:VisualElement>
</engine:UXML>
