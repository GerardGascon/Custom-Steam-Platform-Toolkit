.column__cell {
    flex-grow: 1;
    flex-direction: row;
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
}

.column__cell__attribute-list {
    flex-grow: 1;
    flex-shrink: 1;
    width: auto;
}

.column__header {
    flex-grow: 1;
    flex-direction: row;
    min-height: 20px;
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
}

.column__cell__warning-image {
    flex-shrink: 0;
    align-self: center;
    width: 20px;
    height: 20px;
}

.column__header__warning-image {
    flex-shrink: 0;
    width: 20px;
    height: 20px;
}

.column__header__warning-container {
    width: 20px;
    align-items: auto;
    align-self: center;
    height: 20px;
}

.column__cell__warning-container {
    flex-shrink: 0;
    width: 20px;
    align-self: center;
}

.column__header__title {
    align-self: center;
    flex-grow: 1;
    margin-left: 2px;
}

.column__cell__kebab-button {
    flex-grow: 0;
    background-color: rgba(0, 0, 0, 0);
    background-image: none;
    width: 15px;
    height: 15px;
    font-size: 17px;
    -unity-font-style: normal;
    border-color: rgba(0, 0, 0, 0);
}

.column__cell__kebab-button-container {
    flex-grow: 0;
    width: 20px;
    height: 20px;
}
